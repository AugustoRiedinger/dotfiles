\begin{document}

    % Title information:
    \maketitle

% - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
% - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -

    % Abstract:
    \begin{abstract}
        $4 
    \end{abstract}

    % Keywords:
    \begin{IEEEkeywords}
        $5 
    \end{IEEEkeywords}

    % Table of contents:
    \tableofcontents

% - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
    \section{$6:}

		$0

\end{document}
endsnippet

